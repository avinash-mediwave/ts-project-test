{
  "name": "ts-project-test",
  "version": "1.0.0",
  "description": "A simple skeleton for projects",
  "main": "src/app.ts",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "prepare": "husky install && npx prisma-generate",
    "lint": "eslint . --ext .ts",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prisma-generate": "npx prisma generate",
    "dev": "nodemon src/app.ts",
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "test": "jest --testTimeout=5000 --detectOpenHandles",
    "test:coverage": "jest --coverage --testTimeout=5000 --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-node": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
    "nanoid": "^3.0.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "run-script-os": "^1.1.6",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
